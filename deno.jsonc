{
  "tasks": {
    "test": "GHJK_LOG=debug deno test --fail-fast --parallel --unstable -A tests/*",
    "cache": "deno cache deps/*",
    "check": "deno run -A check.ts"
  },
  "fmt": {
    "exclude": ["**/*.md"]
