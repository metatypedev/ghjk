{
  "tasks": {
    "test": "deno test --parallel --unstable-worker-options --unstable-kv -A tests/*",
    "cache": "deno cache deps/*",
    "check": "deno run -A check.ts"
  },
  "fmt": {
    "exclude": [
      "**/*.md",
      ".ghjk/**",
      ".deno-dir/**"
    ]
  },
  "lint": {
    "exclude": [
      ".deno-dir/**",
      "ghjk.ts",
      "play.ts"
    ],
    "rules": {
      "exclude": [
        "no-explicit-any"
      ]
    }
  }
}
