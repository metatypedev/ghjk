{
  "tasks": {
    "test": "deno test --fail-fast --parallel --unstable -A tests/*",
    "cache": "deno cache deps/*",
    "check": "deno run -A check.ts"
  },
  "fmt": {
    "exclude": ["gh_action/*.js"]
  },
  "lint": {
    "exclude": ["gh_action/*", "ghjk.ts", "play.ts"],
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  }
}
