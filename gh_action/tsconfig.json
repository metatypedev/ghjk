{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "rootDir": "./",
    "moduleResolution": "NodeNext",
    "baseUrl": "./",
    "sourceMap": true,
    "outDir": "./",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "newLine": "lf"
  },
  "exclude": ["./node_modules", "ghjk.ts"]
}
